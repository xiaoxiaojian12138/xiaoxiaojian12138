1-100的奇数
#define  _CRT_SECURE_NO_WARNINGS 
#include<stdio.h>
#include<string.h>
int main()
{
	int a = 0;
	while (a < 100)
	{
		if (1 == a%2)
			printf("%d\n", a);
		else
			;
		a++;
	}
    return 0;
}
判断一个数为奇数
#define  _CRT_SECURE_NO_WARNINGS 
#include<stdio.h>
#include<string.h>
int main()
{
	int a = 0;
	int b=a%2;
  if(b==1)
  printf("是奇数"\n);
  else
  printf("不是奇数"\n);
  }
    return 0;
}
